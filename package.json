{
	"name": "front-end",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"autoprefixer": "^10.3.7",
		"axios": "^0.24.0",
		"lodash": "^4.17.21",
		"postcss-cli": "^9.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.3.0",
		"react-scripts": "4.0.3",
		"tailwindcss": "^2.2.17",
		"use-debounce": "^7.0.0",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"prod": "set REACT_APP_ENV=prod && npm start",
		"dev": "set REACT_APP_ENV=dev && npm start",
		"start": "npm run build:css && react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"build:css": "postcss src/css/tailwind.css -o src/css/main.css",
		"build:watch": "postcss src/css/tailwind.css -o src/css/main.css --watch"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
